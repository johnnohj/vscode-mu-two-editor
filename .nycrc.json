{
	"extends": "@istanbuljs/nyc-config-typescript",
	"all": true,
	"check-coverage": true,
	"branches": 80,
	"functions": 80,
	"lines": 80,
	"statements": 80,
	"include": [
		"src/**/*.ts"
	],
	"exclude": [
		"src/**/*.d.ts",
		"test/**/*",
		"out/**/*",
		"dist/**/*",
		"views/**/*"
	],
	"reporter": [
		"text",
		"html",
		"lcov"
	],
	"report-dir": "coverage"
}